[
    {
        "id": "selection",
        "children":
            [
                { "caption": "-" },
                {
                    "caption": "Change surroundings",
                    "command": "surround_change",
                },
                {
                    "caption": "Delete surroundings",
                    "command": "surround_delete",
                },
            ]
    },
    {
        "id": "preferences",
        "children":
        [
            {
                "id": "package-settings",
                "children":
